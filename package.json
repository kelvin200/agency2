    "react-dom": "18.0.0",
    "react-i18next": "11.16.6",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
