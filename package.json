  "workspaces": {
    "packages": [
      "apps/admin/code",
      "apps/website/code",
      "apps/theme",
      "api/code/dynamoToElastic",
      "api/code/fileManager/*",
      "api/code/graphql",
      "api/code/headlessCMS",
      "api/code/pageBuilder/updateSettings",
      "api/code/pageBuilder/importPages/*",
      "api/code/pageBuilder/exportPages/*",
      "api/code/prerenderingService/render",
      "api/code/prerenderingService/flush",
      "api/code/prerenderingService/queue/*",
      "packages/app/*",
      "packages/react-router/*",
      "packages/*",
      "packages/*",
      "clients/*",
      "clients/athena/apps/admin/code",
      "clients/ninjaw/frontend/website/code"
    ]
  },
  "scripts": {
    "test": "jest --config jest.config.js --passWithNoTests",
    "test:unit": "cross-env TEST_TYPE=unit yarn test",
    "test:integration": "cross-env TEST_TYPE=integration yarn test",
    "test:e2e": "cross-env TEST_TYPE=e2e yarn test",
    "eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "eslint:fix": "yarn eslint --fix",
    "prettier": "prettier \"**/**/*.{js,jsx,ts,tsx,json}\" --config .prettierrc",
    "prettier:check": "yarn prettier --check",
    "prettier:fix": "yarn prettier --write",
    "setup-project": "node ./scripts/setupProject.js",
    "setup-env-files": "node ./scripts/setupEnvFiles.js",
    "link-workspaces": "node ./scripts/linkWorkspaces.js",
    "postinstall": "yarn link-workspaces",
    "deploy:graphql": "cd api/code/graphql && yarn build --env=dev && cd ../../../ && yarn webiny deploy api --env=dev --build=false",
    "deploy:graphql_dynamotoes": "cd api/code/graphql && yarn build --env=dev && cd ../dynamoToElastic && yarn build --env=dev && cd ../../../ && yarn webiny deploy api --env=dev --build=false",
    "start:athena": "cd clients/athena/apps/admin/code && yarn watch --env=dev",
    "start:ninjaw": "cd clients/ninjaw/frontend/website/code && yarn watch --env=dev"
  },
  "resolutions": {
    "@types/react": "17.0.44",
    "react": "18.0.0",
    "typescript": "4.6.3",
    "react-dom": "18.0.0",
